<h1>Add New Restaurant</h1>
<%= form_with(model: @restaurant, url: restaurants_path) do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :address %>
    <%= form.text_field :address %>
  </div>
  <div>
    <%= form.label :category %>
    <%= form.text_field :category %>
  </div>
  <div>
    <%= form.submit 'Create Restaurant' %>
  </div>
<% end %>
